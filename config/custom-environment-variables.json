{
  "openapiConfig": {
    "filePath": "OPENAPI_FILE_PATH"
  },
  "telemetry": {
    "serviceName": "TELEMETRY_SERVICE_NAME",
    "hostname": "TELEMETRY_HOST_NAME",
    "version": "TELEMETRY_SERVICE_VERSION",
    "logger": {
      "level": "LOG_LEVEL",
      "prettyPrint": {
        "__name": "LOG_PRETTY_PRINT_ENABLED",
        "__format": "boolean"
      }
    },
    "tracing": {
      "enabled": "TELEMETRY_TRACING_ENABLED",
      "url": "TELEMETRY_TRACING_URL"
    },
    "metrics": {
      "enabled": "TELEMETRY_METRICS_ENABLED",
      "url": "TELEMETRY_METRICS_URL",
      "interval": "TELEMETRY_METRICS_INTERVAL"
    }
  },
  "server": {
    "port": "SERVER_PORT",
    "request": {
      "payload": {
        "limit": "REQUEST_PAYLOAD_LIMIT"
      }
    },
    "response": {
      "compression": {
        "enabled": {
          "__name": "RESPONSE_COMPRESSION_ENABLED",
          "__format": "boolean"
        }
      }
    }
  },
  "db": {
    "host": "DB_HOST",
    "port": {
      "__name": "DB_PORT",
      "__format": "number"
    },
    "user": "DB_USERNAME",
    "password": "DB_PASSWORD",
    "schema": "DB_SCHEMA",
    "monitorStateIntervalSeconds": {
      "__name": "DB_MONITOR_STATE_INTERVAL_SECONDS",
      "__format": "number"
    },
    "enableSslAuth": {
      "__name": "DB_ENABLE_SSL_AUTH",
      "__format": "boolean"
    },
    "sslPaths": {
      "ca": "DB_CA_PATH",
      "key": "DB_KEY_PATH",
      "cert": "DB_CERT_PATH"
    },
    "database": "DB_NAME",
    "certSecertName": "DB_CERT_SECRET_NAME",
    "deleteAfterHours": {
      "__name": "DB_DELETE_JOBS_AFTER_HOURS",
      "__format": "number"
    }
  },
  "app": {
    "instanceUid": "APP_INSTANCE_UID",
    "jobsConfigPath": "APP_JOBS_CONFIG_PATH"
  },
  "kubernetes": {
    "namespace": "K8S_NAMESPACE",
    "pullSecret": "K8S_PULL_SECRET",
    "loadConfigFromCluster": {
      "__name": "K8S_LOAD_CONFIG_FROM_CLUSTER",
      "__format": "boolean"
    }
  }
}
