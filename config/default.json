{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "app": {
    "instanceUid": "aaaaaa",
    "jobsConfigPath": "./jobs.json"
  },
  "db": {
    "host": "localhost",
    "port": 5432,
    "user": "postgres",
    "password": "postgres",
    "schema": "pgboss",
    "monitorStateIntervalSeconds": 30,
    "enableSslAuth": false,
    "sslPaths": {
      "ca": "",
      "key": "",
      "cert": ""
    },
    "database": "postgres",
    "certSecretName": "",
    "deleteAfterHours": 720
  },
  "kubernetes": {
    "namespace": "default",
    "loadConfigFromCluster": false
  }
}
